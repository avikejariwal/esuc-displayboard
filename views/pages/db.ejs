<!DOCTYPE html>
<html>
<head>
	<% include ../partials/header.ejs %>
	<!--CDN-->
	<script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
	<script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
	<!--BOOTSTRAP-->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
	<!--CSS-->
	<link rel="stylesheet" type="text/css" href="/stylesheets/main.css" />
	<link rel="stylesheet" type='text/css' href="/slick/slick.css" />
	<link rel="stylesheet" type='text/css' href='/slick/slick-theme.css' />
	<!--FIREBASE-->
	<script src="https://cdn.firebase.com/js/client/2.3.2/firebase.js"></script>
	<!--LOCAL JS-->
	<script type="text/javascript" src="/slick/slick.min.js"></script>
	<script src="/js/flierDb.js" type="text/javascript"></script>
</head>

<body>	
	<div >
		<h1>  Upcoming Events</h1>
		<div class="eventInfoContainer">
			
			<ul>
				<div class="list-group">
					<% for (var i = 0; i<events.length; i++) { %>
					<a href="#" class="list-group-item">
						<h4 class="list-group-item-heading"><%= events[i].eventName %></h4>
						<p class="list-group-item-text"><%= events[i].organizationName %></p>
						<p class="list-group-item-text"><%= events[i].dateAndTime.toUTCString() %></p>
						<p class="list-group-item-text"><%= events[i].location %></p>
						<% } %>
					</a>
				</div>
			</ul>

		</div>
		<div class="flierContainer">
			<div class="your-class">
				<% for (var i =0 ; i<fliers.length ; i++) {%>
				<div><img src = <%= fliers[i] %> ></div>
				<% } %>
			</div>
			
		</div>
	</div>

</body>
</html>
